{
  "compilerOptions": {
    "jsxImportSource": "@emotion/react",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "typeRoots": ["./node_modules/@types"],
    "baseUrl": "src",
    "paths": {
      "src/*": ["src/*"],
      "Common": ["Common"]
    }
  },
  "exclude": ["node_modules"],
  "include": ["src", "types", "custom.d.ts"]
}
